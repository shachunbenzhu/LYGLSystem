<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.hibernate.test.orphan">
 <class name="com.cn.ncist.rq.user.vo.AdminModel"  table="tb_admin">
  <id name="admin_id" column="admin_id" type="integer">
  	<generator class="increment"></generator>
  </id>
  <property name="admin_name" column="admin_name" type="string" length="50" not-null="true"></property>
  <property name="admin_pwd" column="admin_pwd" type="string" length="50" not-null="true"></property>
  <property name="admin_question" column="admin_question" type="string" length="100"></property>
  <property name="admin_answer" column="admin_answer" type="string" length="100"></property>
 </class>
</hibernate-mapping>

